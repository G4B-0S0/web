{
    "functions": {

        "api/index.php": { "runtime": "vercel-php@0.7.1" }

    },

    "routes": [

        {
          "src": "/(css|js)/(.*)",
          "dest": "src/$1/$2"
        },

        {
          "src": "src/images/(.*)",
          "dest": "src/images/$1"
        },

        {
          "src": "/index.php",
          "dest": "api/index.php"
        }

    ]

  }